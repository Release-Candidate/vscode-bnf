{
	"name": "vscode-bnf",
	"displayName": "vscode-bnf",
	"description": "BNF for Visual Studio Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Vallentin/vscode-bnf"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "bnf",
				"aliases": ["BNF", "bnf"],
				"extensions": [".bnf", ".ebnf"],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "bnf",
				"scopeName": "source.bnf",
				"path": "./syntaxes/bnf.tmLanguage.json"
			}
		]
	}
}